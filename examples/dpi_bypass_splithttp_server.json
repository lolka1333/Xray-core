{
  "log": {
    "loglevel": "info"
  },
  "inbounds": [
    {
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "your-uuid-here"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "splithttp",
        "security": "tls",
        "tlsSettings": {
          "certificates": [
            {
              "certificateFile": "/path/to/certificate.crt",
              "keyFile": "/path/to/private.key"
            }
          ]
        },
        "splithttpSettings": {
          "path": "/splithttp",
          "mode": "packet-up",
          "scMaxEachPostBytes": {
            "from": 15000,
            "to": 18000
          },
          "scMinPostsIntervalMs": {
            "from": 10,
            "to": 50
          },
          "dpiBypassEnabled": true,
          "dpiFragmentSize": 15360,
          "dpiFragmentDelay": 10,
          "dpiRandomSize": true,
          "dpiMinSize": 10240,
          "dpiMaxSize": 20480
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom"
    }
  ]
}